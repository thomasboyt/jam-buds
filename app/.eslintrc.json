{
    "extends": [
        "plugin:vue/strongly-recommended",
        "eslint:recommended",
        "prettier",
        "prettier/vue"
    ],
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",

        "vue/require-default-prop": "off",
        "vue/require-prop-types": "off",
        "vue/order-in-components": "error"
    }
}